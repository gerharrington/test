              },
              {
                "name": "Gee - The Crows",
                "url": "https://cdn.discordapp.com/attachments/1098488379575914516/1120194662850904184/The_Crows_-_Gee.mp3"
              },
              {
                "name": "Rose Rose I Love You - Frankie Laine",
                "url": "https://cdn.discordapp.com/attachments/1098488379575914516/1130217674501263460/y2mate.com_-_FRANKIE_LAINE_ROSE_ROSE_I_LOVE_YOU.mp3"
              },
              {
                "name": "Hawaiian War Chant - Ames Brothers",
                "url": "https://cdn.discordapp.com/attachments/1098488379575914516/1132067009136443593/YT2mp3.info_-_1951_HITS_ARCHIVE_Hawaiian_War_Chant_-_Ames_Brothers_128kbps.mp3"
              },
              {
                "name": "A Trumpeter’s Lullaby - Leroy Anderson",
                "url": "https://cdn.discordapp.com/attachments/1098488379575914516/1132067376607809596/YT2mp3.info_-_1951_HITS_ARCHIVE_A_Trumpeters_Lullaby_-_Leroy_Anderson_his_original_version_128kbps.mp3"
              }
          ]
        },
               1: {
          name: "Sids Jukebox",
          tracklist: [
            {
               "name": "Walking My Baby Back Home - Johnny Ray",
                "url": "https://cdn.discordapp.com/attachments/1098488379575914516/1130211682958971031/y2mate.com_-_Walking_My_Baby_Back_Home_JOHNNY_RAY.mp3"
            },
            {
                "name": "Blue Tango - Leroy Anderson",
                "url": "https://cdn.discordapp.com/attachments/1098488379575914516/1130217726003130448/y2mate.com_-_1952_HITS_ARCHIVE_Blue_Tango_Leroy_Anderson_his_original_1_version.mp3"
            },
            {
